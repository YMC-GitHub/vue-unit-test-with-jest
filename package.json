{
  "name": "vue-unit-test-with-jest",
  "description": "starter for vue unit test with jest",
  "version": "1.0.0",
  "author": "yemiancheng <ymc-github@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "jest",
    "test:unit": "jest --config test/unit/jest.config.js",
    "test:unit:with-coverage": "jest --config test/unit/jest.config.js --coverage"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.30",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^0.2.0",
    "regenerator-runtime": "^0.13.3",
    "url-loader": "^0.5.9",
    "vue-jest": "^2.6.0",
    "vue-loader": "^13.7.3",
    "vue-server-renderer": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^3.1.11"
  },
  "dependencies": {
    "vue": "^2.6.11"
  }
}